{
  "name": "electron-react-python-app",
  "version": "1.0.0",
  "description": "Electron app with React frontend and Python backend",
  "main": "src/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:python\" \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:python": "python src/backend/main.py",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "vite build",
    "build:electron": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "build": {
    "appId": "com.electron.react.python.app",
    "files": [
      "dist/**/*",
      "src/main.js",
      "src/preload.js",
      "src/backend/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  }
}
